<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kfa">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_main"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_main_round"
        android:windowSoftInputMode="adjustResize"
        android:supportsRtl="true"
        android:theme="@style/Theme.KFA"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.KFA.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".depthActivity"
            android:label="depth"></activity>
        <activity
            android:name=".GameActivity"
            android:label="game_detail"></activity>
        <activity
            android:name=".NewsActivity"
            android:label="NewsActivity"></activity>
        <activity
            android:name=".communityActivity"
            android:label="community"></activity>
        <activity
            android:name=".uploadActivity"
            android:label="community_upload"></activity>
        <activity android:name=".communityPostActivity"
            android:label="community_post"></activity>
    </application>
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>


</manifest>